@model WM.Northwind.Entities.ComplexTypes.IlacTakip.EczaneGrupDetay

@{
    ViewBag.Title = "Delete";
}

<h4>Kayıt Sil</h4>

<h3>Bu kayıt silinecek! Emin misiniz?</h3>
<div>
    <h4>Eczane Grup</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.GrupAdi)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EczaneAdi)
        </dd>


    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sil" class="btn btn-outline-danger" />
            @Html.ActionLink("Eczane Grup Listesi", "Index", null, new { @class = "btn btn-outline-warning" })

        </div>
    }
</div>
